function InputFieldScanner(a){this.form=a;this.fields=null}InputFieldScanner.prototype=new Processing;InputFieldScanner.prototype.constructor=InputFieldScanner;InputFieldScanner.superclass=Processing.prototype;InputFieldScanner.prototype.startProcessing=function(){this.scan();this.doneProcessing()};
InputFieldScanner.prototype.scan=function(){if(null==this.fields){this.fields=[];for(var a=0;a<this.form.elements.length;a++){var b=this.form.elements[a];if("textarea"==b.type||"text"==b.type)this.fields[this.fields.length]=b}}};InputFieldScanner.prototype.getProcessingResults=function(){return this.fields};